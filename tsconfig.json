{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "isolatedModules": false,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "src",
    "paths": {
      "src/*": ["./*"],
      "components": ["./src/components"],
      "screens": ["./src/screens"],
      "constants": ["./src/constants"],
      "navigators": ["./src/navigators"]
    },
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "rn-cli.config.js"
  ]
}
